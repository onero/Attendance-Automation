<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane fx:id="borderPane" prefHeight="600" prefWidth="900" stylesheets="@../../css/Style.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="attendanceautomation.gui.views.rootView.controller.RootViewController">
   <bottom>
      <GridPane stylesheets="@../../css/Style.css" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="169.0" minWidth="10.0" prefWidth="130.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="10.0" prefWidth="27.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="143.0" minWidth="10.0" prefWidth="143.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#handleStartView" prefHeight="31.0" prefWidth="136.0" text="START" />
            <Button fx:id="allStudentsButton" mnemonicParsing="false" onAction="#handleAllStudentsButton" prefHeight="31.0" prefWidth="136.0" text="Alle Studerende" GridPane.columnIndex="2" />
         </children>
         <styleClass>
            <String fx:value="padding" />
            <String fx:value="mainBackground" />
         </styleClass>
      </GridPane>
   </bottom>
</BorderPane>
